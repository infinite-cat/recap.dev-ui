    "@material-ui/lab": "^4.0.0-alpha.57",
    "@material-ui/pickers": "^4.0.0-alpha.12",
    "graphql-type-json": "^0.3.2",
    "graphql.macro": "^1.4.2",
    "lodash-es": "^4.17.20",
