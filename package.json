    "@storybook/addon-knobs": "^6.1.11",
    "@storybook/addon-links": "^6.1.11",
    "@storybook/addon-options": "^5.3.21",
    "@storybook/addon-storyshots": "^6.1.11",
    "@storybook/addon-viewport": "^6.1.11",
    "@storybook/addons": "^6.1.11",
