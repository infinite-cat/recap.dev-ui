{
  "name": "recap.dev-ui",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:8081/",
  "license": "MIT",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "tsc": "tsc",
    "lint": "eslint \"src/**\"",
    "format": "prettier --write ./src",
